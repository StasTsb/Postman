							"var result2 = +(jsonData.salary[2]);\r",
							"\r",
							"pm.test(\"SalaryCheck0\", function () {\r",
							"    pm.expect(jsonData.salary[0]).to.eql(6000);\r",
							"});\r",
							"\r",
							"pm.test(\"SalaryCheck1\", function () {\r",
							"    pm.expect(result).to.eql(12000);\r",
							"});\r",
							"\r",
							"pm.test(\"SalaryCheck2\", function () {\r",
							"    pm.expect(result2).to.eql(18000);\r",
							"});\r",
							"\r",
							"pm.test(\"MoreLess\", function () {\r",
							"    pm.expect(result2>result).to.eql(true);\r",
							"});\r",
							"\r",
							"pm.test(\"MoreLess2\", function () {\r",
							"    pm.expect(result2>result0).to.eql(true);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
